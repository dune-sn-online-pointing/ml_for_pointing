# HTCondor submit file for Channel Tagging training (100k samples, NEW data)
universe = vanilla
executable = scripts/condor_wrapper_channel_tagging.sh
arguments = X '' json/channel_tagging/production_v5.json
+RequestCpus = 1
request_cpus = 1
request_memory = 80GB
request_disk = 20GB
+MaxRunTime = 86400

# Logs
log = logs/channel_tagging_v5_$(Cluster).log
output = logs/channel_tagging_v5_$(Cluster).out
error = logs/channel_tagging_v5_$(Cluster).err

queue 1
