# HTCondor submit file for Channel Tagging v8 (streaming, hyperopt, balanced, 50k samples)
universe = vanilla
executable = scripts/condor_wrapper_channel_tagging.sh
arguments = X '' json/channel_tagging/production_v8_streaming.json
+RequestCpus = 1
request_cpus = 1
request_memory = 4GB
request_disk = 20GB
+MaxRuntime = 86400

# Logs
log = logs/channel_tagging_v8_$(Cluster).log
output = logs/channel_tagging_v8_$(Cluster).out
error = logs/channel_tagging_v8_$(Cluster).err

queue 1
