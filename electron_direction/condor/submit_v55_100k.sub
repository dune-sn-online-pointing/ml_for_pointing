#!/bin/bash
# HTCondor submit file for ED v55 (100k samples, scaled v14 architecture)

executable = wrapper_v55.sh
arguments  = -j json/v55_100k.json

output = logs/ed_v55_100k_$(ClusterId).out
error  = logs/ed_v55_100k_$(ClusterId).err
log    = logs/ed_v55_100k_$(ClusterId).log

request_gpus   = 1
request_cpus   = 4  
request_memory = 16GB
+JobFlavour    = "tomorrow"

queue 1
